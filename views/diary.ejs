<%- include('./layout/Header.ejs')%>
    <div class="container text-center mt-3 p-5 ">
        <h1>Welcome to  <span class="text-success">Express Diary</span></h1>
        <p>Record your dialy Life</p>
    </div>
    <div class="container p-2 bg-info text-light">
        <h4 class="">Add to Diary  <span class="float-sm-end"><a href="/add" class="btn btn-success  ">Add Diary</a></span></h4>
    </div>
   <div class="container mt-3">
    <div class="row ">
        <% data.forEach((data) => {%>
            <div class="col-md-4">
                <div class="card w-75 bg-info-subtle  p-3 m-2">
                    <h3><%= data.title %></h3>
                    <p><%= data.date %></p>
                    <a href="/diary/<%=data.id%>" class="btn btn-primary  w-50 m-3">More</a>
                </div>
                
            </div>
        <%})%>
    </div>

   </div>
    
    <%- include('./layout/Footer.ejs')%>
